{
  "manifest_version": 2,
  "name": "Rabbit Hole",
  "version": "1.0",
  "description": "A Firefox Extension to trace your Wikipedia Journeys",
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": ["storage", "webNavigation", "tabs"],
  "browser_action": {
    "default_title": "Rabbit Hole"
  },
  "content_security_policy": "script-src https://unpkg.com https://*.jquery.com https://*.bootstrapcdn.com 'self' 'unsafe-eval'; object-src 'self'"
}
